{
  "manifest_version": 3,
  "name": "ADBS",
  "version": "0.0.0",
  "description": "ADBS Firefox Addon",
  "permissions": [
    "bookmarks",
    "storage",
    "tabs"
  ],
  "background": {
    "scripts": ["helpers/api.js", "helpers/storage.js", "background.js"]
  },
  "action": {
    "default_popup": "popup.html"
  }
}
