{
  "manifest_version": 3,
  "name": "ADBS",
  "version": "0.0.1",
  "description": "ADBS Firefox Addon",
  "permissions": [
    "bookmarks",
    "storage",
    "tabs"
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "action": {
    "default_popup": "popup/popup.html"
  },
  "web_accessible_resources": [{
    "resources": ["assets/*"],
    "matches": ["<all_urls>"]
  }]
}
